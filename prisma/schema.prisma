generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model LinkMap {
  id        Int       @id @default(autoincrement())
  user_ip   String
  link      String    @unique
  key       String    @unique
  expire_at DateTime?
}
